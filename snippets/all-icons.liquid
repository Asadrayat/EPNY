{% case name %}
  {% when 'left_arrow' %}
    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
      <path d="M19 12L5 12M5 12L12 19M5 12L12 5" stroke="#333333" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
    </svg>
  {% when 'right_arrow' %}
    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="25" viewBox="0 0 24 25" fill="none">
      <path d="M5 12.3H19M19 12.3L12 5.30005M19 12.3L12 19.3" stroke="black" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
    </svg>
  {% when 'close' %}
    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
      <path d="M18 6L6 18M6 6L18 18" stroke="#000" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
    </svg>
  {% when 'faq-plus' %}
    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
      <path d="M12 6V12M12 12V18M12 12H18M12 12L6 12" stroke="#4B5563" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
    </svg>
  {% when 'faq-minus' %}
    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
      <path d="M18 12H6" stroke="#4B5563" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
    </svg>
  {% when 'plus' %}
    <svg class="plus-icon" xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16" fill="none">
      <path d="M3.33331 7.99999L12.6666 7.99999M7.99998 12.6667L7.99998 3.33332" stroke="black" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
    </svg>
  {% when 'minus' %}
    <svg
      class="minus-icon-new"
      xmlns="http://www.w3.org/2000/svg"
      width="16"
      height="16"
      viewBox="0 0 16 16"
      fill="none"
    >
      <path d="M3.33331 8H12.6666" stroke="black" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
    </svg>
  {% when 'account' %}
    <svg xmlns="http://www.w3.org/2000/svg" width="21" height="20" viewBox="0 0 21 20" fill="none">
      <path d="M17.1666 17.5C17.1666 16.337 17.1666 15.7555 17.0231 15.2824C16.6999 14.217 15.8663 13.3834 14.8009 13.0602C14.3278 12.9167 13.7463 12.9167 12.5833 12.9167H8.41665C7.25368 12.9167 6.67219 12.9167 6.19903 13.0602C5.13369 13.3834 4.30001 14.217 3.97685 15.2824C3.83331 15.7555 3.83331 16.337 3.83331 17.5M14.25 6.25C14.25 8.32107 12.571 10 10.5 10C8.42891 10 6.74998 8.32107 6.74998 6.25C6.74998 4.17893 8.42891 2.5 10.5 2.5C12.571 2.5 14.25 4.17893 14.25 6.25Z" stroke="black" stroke-width="1.2" stroke-linecap="round" stroke-linejoin="round"/>
    </svg>
  {% when 'down' %}
    <svg fill="#000000" width="25px" height="25px" viewBox="0 -6 524 524" xmlns="http://www.w3.org/2000/svg">
      <title>down</title><path d="M64 191L98 157 262 320 426 157 460 191 262 387 64 191Z" />
    </svg>
{% endcase %}
